{
  "manifest_version": 3,
  "name": "Tabular",
  "version": "0.0.1",
  "minimum_chrome_version": "114",
  "permissions": [
    "sidePanel", "tabs"
  ],
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "side_panel": {
    "default_path": "side_panel/side_panel.html"
  }
}